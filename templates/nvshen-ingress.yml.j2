---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: "nvshen"
  annotations:
    kubernetes.io/ingress.class: traefik
spec:
  rules:
    - host: "immissi.cn"
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: nvshen
                port:
                  number: 80
    - host: "immissi.com"
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: nvshen
                port:
                  number: 80
    - host: "youyanvren.com.cn"
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: nvshen
                port:
                  number: 80
